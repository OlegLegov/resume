<main>
  <div class='nav row'>
    {{#with navigation}}
      {{#each links}}
        <div class='col-2'>
          <div class='card-body'>
            <button
              type='button'
              class='btn btn-link'
              href='{{href}}'
            >{{text}}</button>
          </div>
        </div>
      {{/each}}
      {{#with button}}
        <div class='col-2'>
          <div class='card-body'>
            <button
              type='button'
              class='btn btn-primary'
              href='{{href}}'
            >{{text}}</button>
          </div>
        </div>
      {{/with}}
    {{/with}}
  </div>
  <hr />
  <br />
  <br />
  <div>
    {{#with header}}
      <h1>{{title}}</h1>
      <br />
      <p>{{description}}</p>
      {{#with button}}
        <button
          type='button'
          class='btn btn-primary'
          href='{{href}}'
        >{{text}}</button>
      {{/with}}
    {{/with}}
  </div>
  <br />
  <br />
  <hr />
  <br />
  <div>
    {{#with city}}
      <h2>{{title}}</h2>
      <br />
      <div class='row gy-5'>
        {{#each cards}}
          <div class='col-5'>
            <div class='card'>
              {{#with picture}}
                <img
                  class='card-img-top'
                  src='{{src}}'
                  alt='{{alt}}'
                />
              {{/with}}
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p class='card-text'>{{description}}</p>
                {{#with button}}
                  <a
                    href='{{href}}'
                    class='btn btn-primary'
                  >{{text}}</a>
                {{/with}}
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <br />
  <div>
    {{#with winner}}
      <h2>{{title}}</h2>
      <br />
      <hr />
      <table class='table'>
        {{#with table}}
          <thead>
            <tr>
              {{#each head}}
                <th>{{this}}</th>
              {{/each}}
            </tr>
          </thead>
          <tbody>
            {{#each user}}
              <tr>
                <th>{{place}}</th>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
              </tr>
            {{/each}}
          </tbody>
        {{/with}}
      </table>
      <br />
    {{/with}}
  </div>
  <hr />
  <br />
  <div>
    {{#with gallery}}
      <h2>{{title}}</h2>
      <br />
      <div class='row'>
        {{#each cards}}
          <div class='col-4'>
            <div class='card'>
              <div class='card-header'>
                {{name}}
              </div>
              <ul class='list-group'>
                {{#each items}}
                  <li class='list-group-item'>{{this}}</li>
                {{/each}}
              </ul>
            </div>
            <br />
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <hr />
  <br />
  <div>
    {{#with relaxion}}
      <h1 class='display-3'>{{title}}</h1>
      <br />
      <br />
      <div class='row gy-4'>
        {{#each cards}}
          <div class='col-5'>
            <div class='card text-bg-secondary'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p class='card-text'>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <br />
  <hr />
  <div>
    <br />
    <br />
    {{#with rest}}
      <div class='row'>
        {{#each cards}}
          <div class='col-6'>
            <div class='card text-center'>
              <div class='card-header'>
                {{header}}
              </div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p class='card-text'>{{description}}</p>
                <a
                  href='#'
                  class='btn btn-primary'
                >{{button}}</a>
              </div>
              <div class='card-footer text-body-secondary'>
                {{footer}}
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
  </div>
  <br />
  <br />
  <hr />
  <div>
    <br />
    {{#with proposition}}
      <h1 class='display-2'>{{title}}</h1>
      <br />
      <br />
      <div class='row'>
        {{#each card}}
          <div class='col-5'>
            <div class='card text-bg-warning'>
              <div class='card-header'>{{header}}</div>
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p class='card-text'>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    {{/with}}
    <br />
    <br />
  </div>
  <hr />
  <br />
  <div class='footer'>
    {{#with footer}}
      <h5>{{title}}</h5>
      <br />
      <div class='row'>
        <div class='col-3'>
          {{#each navigation}}
            <div class='card-body'>
              <ul class='nav flex-column'>
                <p class='nav-item'>
                  <button
                    type='button'
                    class='btn btn-link'
                    href='{{href}}'
                  >{{text}}</button>
                </p>
              </ul>
            </div>
          {{/each}}
        </div>
        <div class='col-3'>
          {{#each socialMedia}}
            <div class='card-body'>
              <ul class='nav flex-column'>
                <p class='nav-item'>
                  <button
                    type='button'
                    class='btn btn-outline-dark'
                    href='{{link}}'
                  >{{name}}</button>
                </p>
              </ul>
            </div>
          {{/each}}
        </div>
        <div class='col-3'>
          {{#each partners}}
            <div class='card-body'>
              <ul class='nav flex-column'>
                <p class='nav-item'>
                  <button
                    type='button'
                    class='btn btn-outline-info'
                    href='{{link}}'
                  >{{name}}</button>
                </p>
              </ul>
            </div>
          {{/each}}
        </div>
      </div>
    {{/with}}
  </div>
</main>